extends layout

block content
 .w3-card-4
  header.w3-container.w3-teal
    h1 Lista de Classes Nivel 1
  table(class="w3-table-all")
     tr
      th Código
      th Título
     each elem in list
       tr
          td
            a(href='/classe/c'+ elem.codigo) #{elem.codigo}
          td= elem.titulo
 